{
  "expo": {
    "name": "Scrybe",
    "slug": "Scrybe",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/scrybe-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/scrybe-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#cccccc"
      },
      "package": "com.siddhantthegts.Scrybe"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
